web: cd /app && /opt/venv/bin/python manage.py collectstatic --noinput --verbosity 2 && /opt/venv/bin/python manage.py migrate --verbosity 2 && /opt/venv/bin/python manage.py check --deploy && /opt/venv/bin/gunicorn DjangoHUD.wsgi:application --bind 0.0.0.0:$PORT --workers 1 --timeout 120 --access-logfile - --error-logfile - --log-level debug